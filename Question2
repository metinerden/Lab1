# Lab1
package Lab_1;

public class Question2 {

	public static void main(String[] args) {
		
		number(6, 4);
		number(4, 5);
		number(1, 5);
		number(12, 6);
	}

	public static void number(int num1, int num2) {

		if (num1 == 6 || num2 == 6 || num1 + num2 == 6 || num2 - num1 == 6 || num1 - num2 == 6) {

			System.out.println(true);
		} else {
			System.out.println(false);

		}
	}
}
