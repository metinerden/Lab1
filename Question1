package Lab_1;
public class Question1 {

	public static void main(String[] args) {
		stringTimes("Hi",3);
		stringTimes("Hi",2);
		stringTimes("Hi",1);
	}
	public static void stringTimes(String string, int number) {
		
		do {
			System.out.print(string);
			number--;
		}while(1<=number);
	System.out.println("");
	}
	
	
	}


