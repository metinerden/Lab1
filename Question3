# Lab1
package Lab_1;

public class Question3 {

	public static void main(String[] args) {
		
		special(22);
		special(23);
		special(24);
		special(3);
	}

	public static void special(int x) {
		if (x % 11 == 0 || x % 11 == 1) {
			System.out.println(true);
		} else {
			System.out.println(false);
		}

	}
}
